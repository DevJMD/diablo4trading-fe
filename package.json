{
  "name": "@sanctuaryteam/web-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "tsc": "tsc --noEmit",
    "lingui:extract": "lingui extract --clean",
    "lingui:compile": "lingui compile --typescript",
    "prebuild": "yarn run lingui:compile",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "dprint fmt",
    "test:unit": "jest --config=jest.unit.config.ts",
    "test:ui": "jest --config=jest.ui.config.ts",
    "test:coverage": "jest --config=jest.unit.config.ts --coverage",
    "test": "yarn run test:unit && yarn run test:ui"
  },
  "dependencies": {
    "@diablosnaps/assets": "^0.2.5",
    "@diablosnaps/common": "^0.2.5",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/alegreya-sans": "^5.0.5",
    "@lingui/macro": "^4.3.0",
    "@lingui/react": "^4.3.0",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.14.2",
    "@reduxjs/toolkit": "^1.9.5",
    "@sanctuaryteam/shared": "^0.2.0",
    "lz-string": "^1.5.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.14.2",
    "react-string-replace": "^1.1.1",
    "react-virtualized": "^9.22.5",
    "react-window": "^1.8.9"
  },
  "devDependencies": {
    "@lingui/cli": "^4.3.0",
    "@lingui/format-json": "^4.3.0",
    "@lingui/vite-plugin": "^4.3.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-virtualized": "^9.21.22",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "babel-plugin-macros": "^3.1.0",
    "dprint": "^0.40.2",
    "eslint": "^8.45.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-svgr": "^3.2.0"
  }
}
